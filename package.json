{
  "name": "react_resume",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.97",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "lorem-ipsum": "^2.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "yarn compileContent && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "compileContent": "yarn compileSetup && yarn compileWork && yarn compileSkills",
    "compileSetup": "npx ts-node -P tsconfig.automation.json ./automation/compileSetup.ts",
    "compileWork": "npx ts-node -P tsconfig.automation.json ./automation/compileWork.ts",
    "compileSkills": "npx ts-node -P tsconfig.automation.json ./automation/compileSkill.ts",
    "deploy": "chmod +x ./deploy.sh && ./deploy.sh"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/react-helmet": "^6.1.11",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.3.0",
    "json-to-ts": "^2.1.0",
    "prettier": "^3.2.5",
    "react-router-dom": "^6.23.1",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^7.10.0",
    "yaml": "^2.4.5"
  }
}